<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meteor.mapper.PositionMapper">
    <select id="getAllPosition" resultType="com.meteor.pojo.Position">
        select * from position
    </select>
    <select id="getPositionById" parameterType="integer" resultType="com.meteor.pojo.Position">
        select * from position where id=#{id}
    </select>
</mapper>
